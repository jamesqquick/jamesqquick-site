{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "data",
      "path": "[[workspace]]/data"
    },
    {
      "title": "data",
      "path": "[[workspace]]/src/data"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "testimonial",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "name",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "image",
          "type": "image",
          "required": true,
          "isPreviewImage": true
        },
        {
          "name": "shortQuote",
          "type": "string",
          "single": true,
          "required": true
        }
      ]
    },
    {
      "name": "blog",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "coverImage",
          "type": "image",
          "required": true,
          "isPreviewImage": true
        },
        {
          "name": "pubDate",
          "type": "datetime",
          "default": "{{now}}",
          "required": true
        },
        {
          "name": "description",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "youTubeVideoId",
          "type": "string",
          "single": true
        },
        {
          "name": "tags",
          "type": "tags",
          "required": true
        }
      ]
    },
    {
      "name": "course",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "coverImage",
          "type": "image",
          "required": true,
          "isPreviewImage": true
        },
        {
          "name": "pubDate",
          "type": "datetime",
          "default": "{{now}}",
          "required": true
        },
        {
          "name": "description",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "youTubeVideoId",
          "type": "string",
          "single": true
        },
        {
          "name": "featured",
          "type": "boolean"
        },
        {
          "name": "link",
          "type": "string",
          "single": true
        }
      ]
    },
    {
      "name": "talk",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "name": "conferenceName",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "conferenceLocation",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "conferenceLink",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "coverImage",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "name": "conferenceLogo",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "youTubeVideoId",
          "type": "string",
          "single": true
        },
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "pubDate",
          "type": "datetime",
          "default": "{{now}}",
          "required": true
        },
        {
          "name": "slidesLink",
          "type": "string",
          "single": true
        },
        {
          "name": "featured",
          "type": "boolean"
        }
      ]
    }
  ],
  "frontMatter.git.enabled": true
}